{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Product",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "Price": {
      "type": "decimal"
    },
    "Image": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "category": {
      "via": "products",
      "model": "category"
    },
    "order_details": {
      "via": "product",
      "collection": "order-details"
    }
  }
}
